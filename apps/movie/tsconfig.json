    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": false,
        "outDir": "./dist",
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
    }
}